    "e7202abf-1953-45a6-941a-f453d2add145": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<p><strong>Explore Our  Collection<\/strong><\/p>\n{% assign typeColList = '' %}\n{% assign regionColList = '' %}\n{% assign occColList = '' %}\n{% assign workColList = '' %}\n{% assign colorColList = '' %}\n{% assign splColList = '' %}\n{% assign priceColList = '' %}\n{% assign accesColList = '' %}\n\n{% for c in collections %}\n\n{%- if c.products_count >0 and c.title != \"Moumita Ready\" -%}\n                    {% unless c.title contains \"HomePage\" and c.title contains \"New Saree\" and c.title contains \"Baisakhi Collection 2019\"%}\n                      {% unless c.title contains \"New Saree\"%}\n                        {% unless c.title contains \"Baisakhi Collection 2019\"%}\n                            {% unless c.title contains \"Blouse - Front\"%}\n                                {% unless c.title contains \"Shopify Facebook Feed Saree\"%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Type\" %}\n                                     {% assign typeColList = typeColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Region\" %}\n                                     {% assign regionColList = regionColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Occasion\" %}\n                                     {% assign occColList = occColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Color\" %}\n                                     {% assign colorColList = colorColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Special\" %}\n                                     {% assign splColList = splColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Accessory\" %}\n                                     {% assign accesColList = accesColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Work\" %}\n                                     {% assign workColList = workColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n  {%- if c.metafields.custom_collection.collection_category contains \"Price\" %}\n                                     {% assign priceColList = priceColList | append: ',' | append: c.url | append: '|' | append: c.title %}\n  {%- endif -%}\n \n                      {%- endunless -%}\n                      {%- endunless -%}\n                      {%- endunless -%}\n                      {%- endunless -%}\n                      {%- endunless -%}\n\n                  {%- endif -%}\n{% endfor %}\n{% assign typeColList = typeColList  | split: \",\" %}\n\n<p><strong>Collections By Type<\/strong><\/p>\n<p>\n{% for item in typeColList %}\n\n    {% assign typeColListSp = item | split: \"|\" %}\n\n {%- if typeColListSp.first != blank %}\n\n       <a href=\"{{ typeColListSp.first }}\">{{ typeColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n{% assign workColList = workColList  | split: \",\" %}\n\n<p><strong>Collections By Work<\/strong><\/p>\n<p>\n{% for item in workColList %}\n\n    {% assign workColListSp = item | split: \"|\" %}\n\n {%- if workColListSp.first != blank %}\n\n       <a href=\"{{ workColListSp.first }}\">{{ workColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n\n<p>\n{% assign regionColList = regionColList | split: \",\" %}\n<p><strong>Collections By Region<\/strong><\/p>\n{% for item in regionColList %}\n    {% assign regionColListSp = item | split: \"|\" %}\n\n {%- if regionColListSp.first != blank %}\n\n       <a href=\"{{ regionColListSp.first }}\">{{ regionColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n{% assign occColList = occColList | split: \",\" %}\n<p><strong>Collections By Occasion<\/strong><\/p>\n<p>\n{% for item in occColList %}\n    {% assign occColListSp = item | split: \"|\" %}\n\n {%- if occColListSp.first != blank %}\n\n       <a href=\"{{ occColListSp.first }}\">{{ occColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n{% assign colorColList = colorColList | split: \",\" %}\n<p><strong>Collections By Color<\/strong><\/p>\n<p>\n{% for item in colorColList %}\n    {% assign colorColListSp = item | split: \"|\" %}\n\n {%- if colorColListSp.first != blank %}\n\n       <a href=\"{{ colorColListSp.first }}\">{{ colorColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n{% assign priceColList = priceColList | split: \",\" %}\n<p><strong>Collections By Price<\/strong><\/p>\n<p>\n{% for item in priceColList %}\n    {% assign priceColListSp = item | split: \"|\" %}\n\n {%- if priceColListSp.first != blank %}\n\n       <a href=\"{{ priceColListSp.first }}\">{{ priceColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n\n{% assign splColList = splColList | split: \",\" %}\n<p><strong>Special Collections<\/strong><\/p>\n<p>\n{% for item in splColList %}\n    {% assign splColListSp = item | split: \"|\" %}\n\n {%- if splColListSp.first != blank %}\n\n       <a href=\"{{ splColListSp.first }}\">{{ splColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>\n\n {% assign accesColList = accesColList | split: \",\" %}\n<p><strong>Accessory Collections<\/strong><\/p>\n<p>\n{% for item in accesColList %}\n    {% assign accesColListSp = item | split: \"|\" %}\n\n {%- if accesColListSp.first != blank %}\n\n       <a href=\"{{ accesColListSp.first }}\">{{ accesColListSp.last }}<\/a> |\n\n {%- endif -%}\n{% endfor %}\n<\/p>",
