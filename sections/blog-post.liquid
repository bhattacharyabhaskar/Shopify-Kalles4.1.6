          <a href="{{ blog_url }}" class="t4s-blog-button-heading">{{ se_stts.bt_lb }}</a>
        </div>		
        {% endif %}	
      {%- endif -%}
    </div>
    {% if layout_des == "1" %}
      {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
